{
	"$schema": "https://json.schemastore.org/prettierrc",
	"semi": true,
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": true,
	"printWidth": 100,
	"trailingComma": "none",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"singleAttributePerLine": true,
	"htmlWhitespaceSensitivity": "ignore"
}
